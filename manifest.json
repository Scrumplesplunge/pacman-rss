{
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Pacman RSS",
    "default_popup": "popup.html"
  },
  "description": "An RSS reader with minimal permissions.",
  "icons": { "128": "icon.png" },
  "manifest_version": 2,
  "name": "Pacman RSS",
  "optional_permissions": ["*://*/*"],
  "permissions": ["alarms", "contextMenus", "notifications"],
  "version": "0.1.3"
}
